            "isAnonymous": false,
            "contactPhone": "9876543210"
        },
        "complaintTypeMap": {
            "POWER_FAIL": "Unavailability of incoming power supply",
            "COMMUNICATION_FAIL": "Panel Not communicating",
            "SINGLE_PHASE_TRIP": "Circuit Trip",
            "THREE_PHASE_TRIP": "Circuit Trip",
            "LAMP_FAILURE": "Lamp Failures"
        }
    },
    "sourceDb": {
        "options": {
            "requestTimeout": 90000,
            "encrypt": false,
            "enableArithAbort": true
        }
    }
}