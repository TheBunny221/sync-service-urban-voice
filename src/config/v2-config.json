{
    "service": {
        "dryRun": false,
        "isDevelopment": false,
        "oneTimeCatchUp": false,
        "systemUserEmail": "sync@fixsmart.dev"
    },
    "syncRules": {
        "batchSize": 5000,
        "lookbackHours": 48,
        "clientId": "3",
